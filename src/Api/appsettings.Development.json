{
  "AWS": {
    "UseLocalStack": true,
    "ServiceURL": "http://localhost:4566",
    "Region": "eu-west-2",
    "AccessKey": "test",
    "SecretKey": "test"
  },
  "QueueOptions": {
    "IntakeQueueOptions": {
      "Url": "http://localhost:4566/000000000000/identity_service_helper_intake"
    }
  },
  "PolledServices": {
    "AzureB2CSyncService": {
      "CronSchedule": "*/30 * * * * ?"
    }
  },
  "Mongo": {
    "DatabaseUri": "mongodb://127.0.0.1:27017",
    "DatabaseName": "livestock-auth"
  },
  "ConnectionStrings": {
    "Auth": "User ID=postgres;Password=postgres;Host=localhost;Database=livestock_auth;"
  },
  "DetailedErrors": true,
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u4}] ({Application}/{MachineName}/{ThreadId}/{SourceContext}.{Method}) {Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}
